{
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "files.eol": "\n",
    "cSpell.words": [
        "backface",
        "cardgame",
        "UNINIT"
    ],
    "[tailwindcss]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "eslint.format.enable": true,
    "eslint.lintTask.enable": true,
    "eslint.run": "onSave"
}
